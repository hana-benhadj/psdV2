@*First row contains 3 cols*@
<div class="row" style="
    flex-wrap: wrap;
    margin-right: -126px;
    margin-left: -67px;
    /*border: solid;*/
    margin-top: 2%;">
    <div class="col-md-4 col1">
        @Html.Partial("List")
    </div>
    <div class="col-md-4 col1">
        @Html.Partial("ButtonsAndRadio")
        @Html.Partial("PlayerButtons")
